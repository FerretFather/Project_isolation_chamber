{
  // The template which will be used for spawn only
  // Example: 
  // {
  //     "name": "default",
  //     "desc": "",
  //     "file": "default.nbt",
  //     "spawns": "default",
  //     "direction": "south",
  //     "offset": [ 0, 0 ]
  // }
  "spawn": {
      "name": "Spawn",
      "desc": "",
      "file": "default.nbt",
      "spawns": "default",
      "direction": "south"
    },

  "spawns": {
    "default": [
      [ 2, 0, 2 ]
    ]
  },
  
  // The list of templates being available. The first entry is the default template.
  "templates": [
    {
      "name": "Isolation Chamber",
      "file": "the_chamber.nbt",
      "spawns": "default",
      "direction": "south"
    },
{
      "name": "Isolation Chamber 2",
      "file": "isolation_chamber.nbt",
      "spawns": "default",
      "direction": "south"
    }

  ]
}
